<script src="//maps.google.com/maps/api/js?v=3.18&sensor=false&client=&key=&libraries=geometry&language=&hl=&region="></script>
<script src="//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/infobox/1.1.9/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->


<p id="notice"><%= notice %></p>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm-3"></div>
    <div class="col-md-4">
<h1>My Properties</h1>

      <% @properties.each do |property| %>
          <div class="bg-primary" style="padding: 20px; font-family: bold; color: black ">
            <p class= "bg-primary">
              <%= property.photo_url  %>
              <%= link_to property.street_address,property_path(id: property.id), class:  "bg-primary" %>
            </p>
          </div>
          <%=  link_to "show", property_path(:id => property.id) %>
          <%=  link_to "Edit", edit_property_path(:id => property.id) %>
          <%=  link_to "Delete", property_path(:id => property.id) ,method: :delete, data: { confirm: "Are you sure?" } %>
          <br>

    <% end %>

      <%= will_paginate @collection, renderer: BootstrapPagination::Rails %>
<br>
<%= link_to 'Back', lists_path %>
<%= link_to 'New Property', new_property_path %>


</div>
</div>
</div>
</div>

