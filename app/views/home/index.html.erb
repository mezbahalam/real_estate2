
<div class="well">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-2"></div>
      <div class="col-md-6">
        <h1>All Real Estates Users</h1>

<% @users.each do |user| %>
    <div class="row">
      <div class="col-md-5">
        <p><%= image_tag avatar_url(user) %> <%=  "#{full_name(user)}"%></p>
      </div>
        <div id="follow_user" class="col-md-1">
          <%= render :partial => "shared/follow_user", :locals => {:user => user} %>
        </div>
    </div>
    <br>
<% end %>
      </div>
    </div>
  </div>
</div>